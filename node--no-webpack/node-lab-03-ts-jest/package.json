{
  "name": "node-lab-03-ts-jest",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "engines": {
    "node": "10"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/node": "^7.7.4",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-decorators": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-typescript": "^7.7.4",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.14",
    "@typescript-eslint/eslint-plugin": "^2.10.0",
    "@typescript-eslint/parser": "^2.10.0",
    "eslint": "^6.7.2",
    "eslint-plugin-jest": "^23.1.1",
    "jest": "^24.9.0",
    "jest-watch-typeahead": "^0.4.2",
    "nodemon": "^2.0.1",
    "typescript": "^3.7.2"
  },
  "dependencies": {
    "core-js": "2.6.10",
    "regenerator-runtime": "^0.13.3"
  },
  "scripts": {
    "t1": "jest ",
    "t1:w": "jest --watch ",
    "t:coverage": "jest --coverage",
    "t": "jest --watchAll",
    "t:clearCache": "jest --clearCache",
    "t:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand",
    "lint": "eslint --config .eslintrc.js \"src/**/*@(.js|.ts)\"",
    "tsc": "tsc --skipLibCheck",
    "tsc:w": "tsc --skipLibCheck -w",
    "n": "babel-node --extensions '.ts,.js' ",
    "n:w": "nodemon --exec babel-node --extensions '.ts,.js' "
  }
}
